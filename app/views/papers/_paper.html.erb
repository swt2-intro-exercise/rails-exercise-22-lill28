<div id="<%= dom_id paper %>">
  <p>
    <strong>Title:</strong>
    <%= paper.title %>
  </p>

  <p>
    <strong>Venue:</strong>
    <%= paper.venue %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= paper.year %>
  </p>

  <p>
    <%= link_to 'Edit Paper', edit_paper_path(paper) %>
  </p>

  <p>
      <%= link_to 'Delete Paper', paper_path(paper), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>
  </p>

</div>
